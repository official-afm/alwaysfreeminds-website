\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z\"/>\n          </svg>\n        </div>\n        <h3 class=\"text-xl font-bold text-gray-900 mb-3 text-center\">Strategy Session</h3>\n        <div class=\"text-center mb-4\">\n          <span class=\"text-2xl font-bold text-primary-600\">$150</span>\n          <p class=\"text-gray-500 text-sm\">60 minutes</p>\n        </div>\n        <p class=\"text-gray-600 mb-6 text-center\">Deep dive analysis with a custom 30-day action plan to get results fast.</p>\n        <button @click=\"openSavvyCal('strategy')\" class=\"w-full bg-primary-600 text-white py-3 px-6 rounded-lg font-semibold hover:bg-primary-700 transition-colors\">\n          Book Strategy Session\n        </button>\n      </div>\n\n      <!-- Workshop Planning -->\n      <div class=\"bg-white p-8 rounded-xl shadow-lg border border-gray-200 hover:shadow-xl transition-all duration-300\">\n        <div class=\"w-16 h-16 bg-purple-100 rounded-full flex items-center justify-center mx-auto mb-6\">\n          <svg class=\"w-8 h-8 text-purple-600\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z\"/>\n          </svg>\n        </div>\n        <h3 class=\"text-xl font-bold text-gray-900 mb-3 text-center\">Workshop Planning</h3>\n        <div class=\"text-center mb-4\">\n          <span class=\"text-2xl font-bold text-purple-600\">FREE</span>\n          <p class=\"text-gray-500 text-sm\">30 minutes</p>\n        </div>\n        <p class=\"text-gray-600 mb-6 text-center\">Free planning call for workshops starting at $1,500</p>\n        <button @click=\"openSavvyCal('workshop')\" class=\"w-full bg-purple-600 text-white py-3 px-6 rounded-lg font-semibold hover:bg-purple-700 transition-colors\">\n          Plan Workshop\n        </button>\n      </div>\n    </div>\n\n    <!-- Inline Calendar -->\n    <div class=\"max-w-4xl mx-auto\">\n      <div class=\"text-center mb-8\">\n        <h3 class=\"text-2xl font-bold text-gray-900 mb-4\">Select Your Preferred Time</h3>\n        <p class=\"text-lg text-gray-600\">Choose a time that works for you</p>\n      </div>\n      <div class=\"bg-white rounded-2xl shadow-xl p-6\">\n        <div :data-savvycal-embed=\"mainBookingLink\" data-embed-type=\"inline\" data-theme=\"modern\" style=\"min-width:320px;height:600px;\"></div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'SavvyCalBooking',\n  data() {\n    return {\n      savvyCalLinks: {\n        main: 'greg-AlwaysFreeMinds/strategy',\n        discovery: 'greg-AlwaysFreeMinds/chat-with-greg',\n        strategy: 'greg-AlwaysFreeMinds/strategy', \n        workshop: 'greg-AlwaysFreeMinds/workshop'\n      },\n      isLoaded: false\n    }\n  },\n  computed: {\n    mainBookingLink() {\n      return this.savvyCalLinks.main;\n    }\n  },\n  mounted() {\n    this.loadSavvyCalScript();\n  },\n  methods: {\n    loadSavvyCalScript() {\n      if (!document.querySelector('script[src*=\"savvycal\"]')) {\n        const script = document.createElement('script');\n        script.src = 'https://embed.savvycal.com/v1/embed.js';\n        script.async = true;\n        script.onload = () => {\n          this.isLoaded = true;\n          if (window.SavvyCal) window.SavvyCal.init();\n        };\n        document.head.appendChild(script);\n      } else {\n        this.isLoaded = true;\n        if (window.SavvyCal) window.SavvyCal.init();\n      }\n    },\n    openSavvyCal(type) {\n      const link = this.savvyCalLinks[type];\n      if (this.isLoaded && window.SavvyCal) {\n        window.SavvyCal.popup(link);\n      } else {\n        window.open(`https://savvycal.com/${link}`, '_blank', 'width=800,height=700');\n      }\n      console.log(`SavvyCal booking clicked: ${type}`);\n    }\n  }\n}\n</script>\n"],"mappings":"AA0EA,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE;QACbC,IAAI,EAAE,+BAA+B;QACrCC,SAAS,EAAE,qCAAqC;QAChDC,QAAQ,EAAE,+BAA+B;QACzCC,QAAQ,EAAE;MACZ,CAAC;MACDC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACP,aAAa,CAACC,IAAI;IAChC;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACPD,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACE,QAAQ,CAACC,aAAa,CAAC,yBAAyB,CAAC,EAAE;QACtD,MAAMC,MAAK,GAAIF,QAAQ,CAACG,aAAa,CAAC,QAAQ,CAAC;QAC/CD,MAAM,CAACE,GAAE,GAAI,wCAAwC;QACrDF,MAAM,CAACG,KAAI,GAAI,IAAI;QACnBH,MAAM,CAACI,MAAK,GAAI,MAAM;UACpB,IAAI,CAACZ,QAAO,GAAI,IAAI;UACpB,IAAIa,MAAM,CAACC,QAAQ,EAAED,MAAM,CAACC,QAAQ,CAACC,IAAI,CAAC,CAAC;QAC7C,CAAC;QACDT,QAAQ,CAACU,IAAI,CAACC,WAAW,CAACT,MAAM,CAAC;MACnC,OAAO;QACL,IAAI,CAACR,QAAO,GAAI,IAAI;QACpB,IAAIa,MAAM,CAACC,QAAQ,EAAED,MAAM,CAACC,QAAQ,CAACC,IAAI,CAAC,CAAC;MAC7C;IACF,CAAC;IACDG,YAAYA,CAACC,IAAI,EAAE;MACjB,MAAMC,IAAG,GAAI,IAAI,CAACzB,aAAa,CAACwB,IAAI,CAAC;MACrC,IAAI,IAAI,CAACnB,QAAO,IAAKa,MAAM,CAACC,QAAQ,EAAE;QACpCD,MAAM,CAACC,QAAQ,CAACO,KAAK,CAACD,IAAI,CAAC;MAC7B,OAAO;QACLP,MAAM,CAACS,IAAI,CAAC,wBAAwBF,IAAI,EAAE,EAAE,QAAQ,EAAE,sBAAsB,CAAC;MAC/E;MACAG,OAAO,CAACC,GAAG,CAAC,6BAA6BL,IAAI,EAAE,CAAC;IAClD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}