bar',\n  setup() {\n    const mobileMenuOpen = ref(false)\n    \n    const navigation = [\n      { name: 'Home', href: '/' },\n      { name: 'Blog', href: '/blog' },\n      { name: 'Books', href: '/books' },\n      { name: 'Consulting', href: '/consulting' },\n      { name: 'About', href: '/about' },\n    ]\n\n    return {\n      mobileMenuOpen,\n      navigation\n    }\n  }\n}\n</script>"],"mappings":"AAgFA,SAASA,GAAE,QAAS,KAAI;AAExB,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,cAAa,GAAIH,GAAG,CAAC,KAAK;IAEhC,MAAMI,UAAS,GAAI,CACjB;MAAEH,IAAI,EAAE,MAAM;MAAEI,IAAI,EAAE;IAAI,CAAC,EAC3B;MAAEJ,IAAI,EAAE,MAAM;MAAEI,IAAI,EAAE;IAAQ,CAAC,EAC/B;MAAEJ,IAAI,EAAE,OAAO;MAAEI,IAAI,EAAE;IAAS,CAAC,EACjC;MAAEJ,IAAI,EAAE,YAAY;MAAEI,IAAI,EAAE;IAAc,CAAC,EAC3C;MAAEJ,IAAI,EAAE,OAAO;MAAEI,IAAI,EAAE;IAAS,CAAC,CACnC;IAEA,OAAO;MACLF,cAAc;MACdC;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}